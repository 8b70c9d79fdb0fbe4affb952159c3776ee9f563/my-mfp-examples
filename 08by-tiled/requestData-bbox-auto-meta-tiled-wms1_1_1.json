{
  "layout": "Auto meta tiling",
  "outputFormat": "pdf",
  "attributes": {
    "map": {
      "projection": "EPSG:4326",
      "dpi": 72,
      "rotation": 0,
      "bbox": [
        -130,
        20,
        -60,
        50
      ],
      "longitudeFirst": true,
      "layers": [
        {
          "type": "WMS",
          "baseURL": "http://localhost:8088/geoserver/by/wms",
          "opacity": 1,
          "serverType": "geoserver",
          "layers": [
            "by:gemeinden"
          ],
          "imageFormat": "image/png",
          "customParams": {
            "TRANSPARENT": "true"
          }
        }
      ]
    }
  }
}
