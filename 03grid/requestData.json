{
  "layout": "A4 portrait",
  "outputFormat": "png",
  "attributes": {
    "map": {
	"longitudeFirst": true,
	"center": [
	    10.458984,
	    50.331436],
	"scale": 10000000,
      "projection": "EPSG:4326",
      "dpi": 72,
      "rotation": 0,
      "layers": [
        {
          "type": "geojson",
          "geoJson": "file://european-union-countries.geojson",
          "style": {
            "version": "2",
            "*": {
              "symbolizers": [
                {
                  "type": "polygon",
                  "fillColor": "#000099",
                  "fillOpacity": 0.3,
                  "strokeColor": "#CC1D18",
                  "strokeOpacity": 1,
                  "strokeWidth": 1
                },
		 {
                  "type": "text",
                  "label": "[admin]",
                  "haloRadius": "3"
                } 
              ]
            }
          }
        },
          {
              "type": "grid",
              "gridType": "points",
              "numberOfLines": [
		  5,
		  5
              ],
              "renderAsSvg": true,
              "haloColor": "#CCFFCC",
              "labelColor": "black",
              "labelFormat": "%1.0f %s",
              "indent": 10,
              "haloRadius": 4,
              "font": {
		  "name": [
		      "Liberation Sans",
		      "Helvetica",
		      "Nimbus Sans L",
		      "Liberation Sans",
		      "FreeSans",
		      "Sans-serif"
		  ],
		  "size": 8,
		  "style": "BOLD"
              }
          }
      ]
    }
  }
}
